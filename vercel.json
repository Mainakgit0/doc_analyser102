{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "DATABASE_URL": "@database_url",
    "GROQ_API_KEY": "@groq_api_key",
    "SESSION_SECRET": "@session_secret"
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  }
}